<% if !@ticket_categories.nil? %>
	<table class="table table-striped table-responsive" id ="ticket_categories" >
	  <thead>
	    <tr>
		      	<th></th>
	    </tr>
	  </thead>

	  <tbody>
	    <% @ticket_categories.each do |ticket_category| %>
	      <tr>

	        <td>
	        	
		            <%= link_to ticket_category_path(@ticket_category), :class => 'btn btn-primary btn-xs', :title => "#{ t('.show', :default => t('helpers.links.show')) }" do %>
		              <i class ="fa fa-eye"></i>
		            <% end -%>
		        
		            <%= link_to edit_ticket_category_path(@ticket_category), :class => 'btn btn-primary btn-xs', :title => "#{ t('.show', :default => t('helpers.links.show')) }" do %>
		              <i class ="fa fa-edit"></i>
		            <% end -%>
		        
				    <%= link_to ticket_category_path(@ticket_category), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-primary btn-xs', :title => "#{ t('.destroy', :default => t('helpers.links.destroy')) }" do %>
				      <i class ="fa fa-times"></i>
				    <% end -%>
				
	        

	        </td>
	        
	      </tr>
	    <% end %>
	  </tbody>
	</table>
<% end %>